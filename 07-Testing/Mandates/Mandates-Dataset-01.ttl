######################################  POWER #7  ###############################################
:POWER-7 rdf:type :Power ;
    :id [
                ccts:identifierValue "P-7"          ;
                ccts:schemeID "POWER"               ;
                ccts:schemeAgencyID "EU-COM-DIGIT"  ;
                ccts:schemeVersionID "1.1.0"
