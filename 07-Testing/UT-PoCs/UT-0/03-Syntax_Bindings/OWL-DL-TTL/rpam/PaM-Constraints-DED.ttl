#################################################################
#    Individuals
#################################################################

# PIR Constraints

:PowerIRConstraintsScheme rdf:type owl:NamedIndividual, skos:ConceptScheme ;
				skos:prefLabel "Constraints on the EU Power Taxonomy Data Elements."@en .

:ADMINISTRATIVE a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Administrative Constraint"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:definition "Limits the action to certain administrations or administrative acts."@en .

:ADMINISTRATION_LEVEL a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Public Administration Level"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:definition "The level of the Public Administration for which the Mandatee has the power of acting on behalf of the Mandator"@en ;
			skos:broader :ADMINISTRATIVE ;
			skos:hasTopConcept :ADMINISTRATIVE .

:QUANTITATIVE a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Quantitative Constraint"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:definition "Expresses a quantitative restriction applied on a Power data element."@en .

:FINANCIAL  a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Financial Constraint"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:definition "Expresses a financial restriction applied on a Power data element."@en .

:TEMPORAL a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Temporal Constraint"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:definition "Expresses a temporal restriction applied on a Power data element. E.g. and end date, a start date, a period, a quantity of years, months, weeks, days, hours."@en .

:DOCUMENTARY a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Documentary Constraint"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:definition "Expresses a restriction about a type of document, record or evidence required by the Service Provider."@en .

:ABOVE_AGE a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Above Age"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :QUANTITATIVE ;
			skos:broader :QUANTITATIVE ;
			skosxl:prefLabel :AboveAgeType;
			skos:definition "The minimum age required by the Service Provider."@en .

:GET a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Greater Or Equal Than"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :QUANTITATIVE ;
			skos:broader :QUANTITATIVE ;
			skosxl:prefLabel :GreaterOREqualThanType ;
			skos:definition "The minimum quantity required by the Service Provider."@en .

:LET a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Lesser Or Equal Than"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :QUANTITATIVE ;
			skos:broader :QUANTITATIVE ;
			skosxl:prefLabel :LesserOrEqualThanType ;
			skos:definition "The maximum quantity required by the Service Provider."@en .

:MIN_AMOUNT a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Minimum Amount"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :FINANCIAL ;
			skos:broader :FINANCIAL ;
			skosxl:prefLabel :AmountType ;
			skos:definition "The minimum amount required by the Service Provider in a financial operation. The currency should be identified."@en .

:MAX_AMOUNT a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Maximum Amount"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :FINANCIAL ;
			skos:broader :FINANCIAL;
			skosxl:prefLabel :AmountType ;
			skos:definition "The maximum amount required by the Service Provider. The currency should be identified."@en .

:MIN_DATE a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Minimum Date"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :TEMPORAL;
			skos:broader :TEMPORAL ;
			skosxl:prefLabel :DateType ;
			skos:definition "The minimum date specified by the Service Provider."@en .

:MAX_DATE a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Maximum Date"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :TEMPORAL;
			skos:broader :TEMPORAL ;
			skosxl:prefLabel :DateType ;
			skos:definition "The maximum date specified by the Service Provider."@en .

:START_END_DATE a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Start Date End Date"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :TEMPORAL;
			skos:broader :TEMPORAL ;
			skosxl:prefLabel :PeriodStartEndDateType ;
			skos:definition "The period of time specified by the Service Provider as a start date and an end date."@en .

:START_DATE_LAPSE a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Start Date And Lapse"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :TEMPORAL;
			skos:broader :TEMPORAL ;
			skosxl:prefLabel :PeriodLapseType ;
			skos:definition "The period of time specified by the Service Provider as a start date and a quantity of years, months, weeks, etc."@en .

:EVIDENCE_TYPE a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Evidence Type"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :DOCUMENTARY ;
			skos:broader :DOCUMENTARY ;
			skosxl:prefLabel :EvidenceType ;
			skos:definition "Type of evidence."@en .

:EVIDENCE_SCHEME_VERSION a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Evidence Scheme Version"@en ;
			skos:inScheme :PowerIRConstraintsScheme ;
			skos:hasTopConcept :DOCUMENTARY ;
			skos:broader :DOCUMENTARY ;
			skosxl:prefLabel :EvidenceSchemeVersionType ;
			skos:definition "Version number of the scheme defining a type of evidence."@en .

## Constraint Types

:DateType a skosxl:Label ;
	rdf:type skosxl:Label ;
	dct:type xsd:Date;
    rdfs:label "Date Type"@en ;
    skosxl:literalForm "Quantity Type"@en ;
    skos:definition "The type of a date expressed as DD-MM-YYYY"@en .

:PeriodStartEndDateType a skosxl:Label ;
	rdf:type skosxl:Label ;
	dct:type ubl:Period;
    rdfs:label "Start and end date type"@en ;
    skosxl:literalForm "Start and end date type"@en ;
    skos:definition "The type of a period expressed with an start date and end date [DD-MM-YYYY, DD-MM-YYYY]"@en .

:PeriodLapseType a skosxl:Label ;
	rdf:type skosxl:Label ;
	dct:type ubl:Period;
    rdfs:label "Period lapse type"@en ;
    skosxl:literalForm "Period lapse type"@en ;
    skos:definition "The type of a period expressed with an start date and a quantity [DD-MM-YYYY, 2]. The quantity unit-code should be specified (e.g. YEARS, MONTHS, etc."@en .

:QuantityType a skosxl:Label ;
	rdf:type skosxl:Label ;
	dct:type xsd:decimal;
    rdfs:label "Quantity type"@en ;
    skosxl:literalForm "Quantity type"@en ;
    skos:definition "The type of a quantity. The unit to which this quantity refers should be specified, e.g. YEARS, MONTHS, DAYS, KILOGRAMS, LITERS, etc."@en .

:AmountType a skosxl:Label ;
	rdf:type skosxl:Label ;
	dct:type ccts:Amount;
    rdfs:label "Amount type"@en ;
    skosxl:literalForm "Amount type"@en ;
    skos:definition "The type of an economic quantity. The currency can be specified."@en .

:AmountType a skosxl:Label ;
	rdf:type skosxl:Label ;
	dct:type ccts:Amount;
    rdfs:label "Amount type"@en ;
    skosxl:literalForm "Amount type"@en ;
    skos:definition "The type of an economic quantity. The currency can be specified."@en .

# Documentary constraints data element types

:EvidenceType a skosxl:Label ;
	rdf:type skosxl:Label ;
	dct:type skos:Concept;
    rdfs:label "Evidence type"@en ;
    skosxl:literalForm "Evidence type"@en ;
    skos:definition "The type of an evidence (should default to EU_EMANDATE)."@en .

 :EvidenceSchemeVersionType a skosxl:Label ;
	rdf:type skosxl:Label ;
	dct:type xsd:Date;
    rdfs:label "Evidence Scheme Version Type"@en ;
    skosxl:literalForm "Evidence Scheme Version Type"@en ;
    skos:definition "The type of of the data element representing the version of the scheme defining a type of evidence."@en .
