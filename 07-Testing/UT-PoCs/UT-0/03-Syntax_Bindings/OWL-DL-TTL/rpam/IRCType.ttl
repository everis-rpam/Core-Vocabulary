#################################################################
#    Individuals
#################################################################

:InformationRequirementConstraintsScheme rdf:type owl:NamedIndividual, skos:ConceptScheme ;
				skos:prefLabel "Information Requirement and Constraint Types"@en .

:INFORMATION_REQUIREMENT a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Information Requirement"@en ;
			skos:inScheme :InformationRequirementConstraintsScheme ;
			skos:definition "Information Requirement issued by a service provider or a mandator."@en .

:CONSTRAINT  a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Constraint"@en ;
			skos:inScheme :InformationRequirementConstraintsScheme ;
			skos:definition "A limitiation applied by a service provider or a mandator on the use of an information requirement or on the use of a power."@en .

:CAPTION a owl:NamedIndividual, skos:Concept ;
			skos:prefLabel "Caption"@en ;
			skos:inScheme :InformationRequirementConstraintsScheme ;
			skos:definition "A label used to inform on the use of one element inside a group of information requirements and constraints."@en .
